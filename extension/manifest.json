{
    "manifest_version": 2,
    "name": "Kary's Ungamify",
    "author": "Pouya Kary",
    "incognito": "spanning",
    "version": "0.1",
    "description": "Remove gamifications and don't get addicted to stuff!",
    "icons": {
        "48": "icon/icon.png"
    },

    "content_scripts": [
        {
            "matches": ["*://github.com/*"],
            "js": ["sites/github.com.js"]
        },
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["sites/youtube.com.js"]
        }
    ]
  }