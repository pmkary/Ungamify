{
    "manifest_version": 2,
    "name": "Ungamify",
    "version": "1.0",

    "description": "Remove gamifications and don't get addicted to stuff!",

    "icons": {
        "48": "icon/icon.png"
    },

    "content_scripts": [
        {
            "matches": ["*://github.com/*"],
            "js": ["sites/github.com.js"]
        }
    ]
  }